<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ComStyleCategoryDetailMapper">

	<sql id="styleCategoryDetailColumns">styleCategoryDetailId,styleCategoryId,styleCategoryDetailCode,styleCategoryDetailName,styleCategoryDetailStatus,orderNum,effective,createUserId,createTime,modifyUserId,modifyTime</sql>
	<sql id="styleCategoryDetailTable">com_style_category_detail_detail</sql>
	
	<resultMap type="comStyleCategoryDetail" id="comStyleCategoryDetailResultMap">
		<id column="styleCategoryDetailId" property="styleCategoryDetailId"/>
		<result column="styleCategoryDetailCode" property="styleCategoryDetailCode"/>
		<result column="styleCategoryDetailName" property="styleCategoryDetailName"/>
		<result column="styleCategoryDetailStatus" property="styleCategoryDetailStatus"/>
		<result column="orderNum" property="orderNum"/>
		<result column="effective" property="effective"/>
		<result column="createUserId" property="createUserId"/>
		<result column="createTime" property="createTime"/>
		<result column="modifyUserId" property="modifyUserId"/>
		<result column="modifyTime" property="modifyTime"/>
		<result column="styleCategoryId" property="styleCategoryId"/>
	</resultMap>
	
	
	<!-- ****************************custom * start*********************************** -->
	
	<!-- ****************************custom * end  *********************************** -->
	
	
	<!-- ****************************common * satrt*********************************** -->
	<!-- 新增 -->
	<insert id="add" parameterType="comStyleCategoryDetail">
		insert into 
	<include refid="styleCategoryDetailTable"/>
		( 
	<include refid="styleCategoryDetailColumns"/>
		) values (
			#{styleCategoryDetailId},#{styleCategoryId},#{styleCategoryDetailCode},#{styleCategoryDetailName},#{styleCategoryDetailStatus},#{orderNum},#{effective},#{createUserId},#{createTime},#{modifyUserId},#{modifyTime}
		)
	</insert>
	
	<!-- 修改 -->
	<update id="edit" parameterType="comStyleCategoryDetail">
		update
	<include refid="styleCategoryDetailTable"/>
		set 
			<if test="styleCategoryDetailCode!=null and styleCategoryDetailCode!=''">
			styleCategoryDetailCode = #{styleCategoryDetailCode},
			</if>
			<if test="styleCategoryDetailName!=null and styleCategoryDetailName!=''">
			styleCategoryDetailName = #{styleCategoryDetailName},
			</if>
			<if test="orderNum!=null and orderNum!=''">
			orderNum = #{orderNum},
			</if>
			modifyUserId = #{modifyUserId},
			modifyTime = #{modifyTime}
		where 
			styleCategoryDetailId = #{styleCategoryDetailId} 
			and modifyTime = #{lastModifyTime}
	</update>
	
	<!-- 改变 -->
	<update id="change" parameterType="comStyleCategoryDetail">
		update
	<include refid="styleCategoryDetailTable"/>
		set 
			<if test="styleCategoryId!=null and styleCategoryId!=''">
			styleCategoryId = #{styleCategoryId},
			</if>
			<if test="styleCategoryDetailCode!=null and styleCategoryDetailCode!=''">
			styleCategoryDetailCode = #{styleCategoryDetailCode},
			</if>
			<if test="styleCategoryDetailName!=null and styleCategoryDetailName!=''">
			styleCategoryDetailName = #{styleCategoryDetailName},
			</if>
			<if test="styleCategoryDetailStatus!=null and styleCategoryDetailStatus!=''">
			styleCategoryDetailStatus = #{styleCategoryDetailStatus},
			</if>
			<if test="orderNum!=null and orderNum!=''">
			orderNum = #{orderNum},
			</if>
			<if test="effective!=null and effective!=''">
			effective = #{effective},
			</if>
			modifyUserId = #{modifyUserId},
			modifyTime = #{modifyTime}
		where 
			styleCategoryDetailId = #{styleCategoryDetailId} 
			and modifyTime = #{lastModifyTime}
	</update>
	
	<!-- 删除 -->
	<delete id="deleteByPd" parameterType="pd">
		delete from 
	<include refid="styleCategoryDetailTable"/>
		where 
			styleCategoryDetailId = #{styleCategoryDetailId} 
	</delete>
	
	<!-- 批量删除 -->
	<delete id="batchDeleteByIds" parameterType="String">
		delete from 
 <include refid="styleCategoryDetailTable"/>
		where 
			CONCAT(styleCategoryDetailId) in 
        <foreach item="id" index="index" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
	</delete>
	
	<!-- 通过id获取(类)数据 -->
	<select id="findByPd" parameterType="pd" resultType="comStyleCategoryDetail">
		select 
	<include refid="styleCategoryDetailColumns"/>
		from 
	<include refid="styleCategoryDetailTable"/>
		where 
			styleCategoryDetailId = #{styleCategoryDetailId} 
	</select>
	
	<!-- 获取(类)List数据  -->
	<select id="listByPd" parameterType="pd" resultMap="comStyleCategoryDetailResultMap">
		select 
	<include refid="styleCategoryDetailColumns"/>
		from 
	<include refid="styleCategoryDetailTable"/>
		where 1=1
			<if test="styleCategoryId!=null and styleCategoryId!=''">
			and styleCategoryId = #{styleCategoryId}
			</if>
		order by (orderNum+0) 
	</select>
	
	<!-- 获取(类)List数据  -->
	<select id="otherHave" parameterType="comStyleCategoryDetail" resultMap="comStyleCategoryDetailResultMap">
		select 
	<include refid="styleCategoryDetailColumns"/>
		from 
	<include refid="styleCategoryDetailTable"/>
		where 1=1
			<if test="styleCategoryDetailId!=null and styleCategoryDetailId!=''">
			 and styleCategoryDetailId != #{styleCategoryDetailId} 
			</if>
			<if test="styleCategoryId!=null and styleCategoryId!=''">
			and styleCategoryId = #{styleCategoryId}
			</if>
			<if test="styleCategoryDetailCode!=null and styleCategoryDetailCode!=''">
			 and styleCategoryDetailCode = #{styleCategoryDetailCode}
			</if>
			<if test="styleCategoryDetailName!=null and styleCategoryDetailName!=''">
			 and styleCategoryDetailName = #{styleCategoryDetailName}
			</if>
			<if test="styleCategoryDetailStatus!=null and styleCategoryDetailStatus!=''">
			 and styleCategoryDetailStatus = #{styleCategoryDetailStatus}
			</if>
			<if test="orderNum!=null and orderNum!=''">
			 and orderNum = #{orderNum}
			</if>
			<if test="effective!=null and effective!=''">
			 and effective = #{effective}
			</if>
			<if test="createUserId!=null and createUserId!=''">
			 and createUserId = #{createUserId}
			</if>
			<if test="createTime!=null and createTime!=''">
			 and createTime = #{createTime}
			</if>
			<if test="modifyUserId!=null and modifyUserId!=''">
			 and modifyUserId = #{modifyUserId}
			</if>
			<if test="modifyTime!=null and modifyTime!=''">
			 and modifyTime = #{modifyTime}
			</if>
		order by (orderNum+0) 
	</select>
	
	<!-- 通过id获取(PageData)数据  -->
	<select id="listPage" parameterType="bgPage" resultType="pd">
			select 
	<include refid="styleCategoryDetailColumns"/>
		from 
	<include refid="styleCategoryDetailTable"/>
		where 1=1
			and styleCategoryId = #{pd.styleCategoryId}
		<if test="pd.keywords!= null and pd.keywords != ''"><!-- 关键词检索 -->
			and
				(
				styleCategoryDetailCode LIKE CONCAT(CONCAT('%', #{pd.keywords}),'%')
				or
				styleCategoryDetailName LIKE CONCAT(CONCAT('%', #{pd.keywords}),'%')
				)
		</if>
		order by (orderNum+0)
	</select>
	
	<!-- ****************************common * end  ********************************** -->
	
</mapper>